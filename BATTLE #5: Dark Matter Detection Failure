console.log(`Failed detections: ${failed_detections}`);
console.log(`Success rate: ${((total_experiments - failed_detections)/total_experiments*100).toFixed(1)}%`);
console.log(`Failure rate: ${failure_rate.toFixed(1)}%`);
console.log(`Total research years: ${total_years}`);
console.log(`Estimated funding: >$10 billion`);

// Statistical analysis of null results
const detection_probability_per_year = 0.1; // If dark matter existed
const probability_no_detection = Math.pow(1 - detection_probability_per_year, total_years);

