console.log("-----------|-------|-------------|--------|---------------|-------");

let total_experiments = detection_experiments.length;
let failed_detections = 0;
let total_years = 0;

detection_experiments.forEach(exp => {
    const expected = "Detection";
    const status = exp.result.includes("No") ? "FAILED" : "SUCCESS";
    
    if (status === "FAILED") failed_detections++;
    total_years += exp.years;
    
    console.log(`${exp.name.padEnd(10)} | ${exp.years.toString().padEnd(5)} | ${exp.sensitivity.padEnd(11)} | ${exp.result.padEnd(14)} | ${expected.padEnd(13)} | ${status}`);
});

const failure_rate = failed_detections / total_experiments * 100;

